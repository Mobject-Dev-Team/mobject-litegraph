{
  "name": "mobject-litegraph",
  "version": "0.1.7",
  "description": "A flow graph node editor on the web based on a fork or litegraph.js",
  "type": "module",
  "main": "dist/mobject-litegraph.bundle.cjs.js",
  "module": "dist/mobject-litegraph.bundle.esm.js",
  "browser": "dist/mobject-litegraph.bundle.js",
  "exports": {
    ".": {
      "import": "./dist/mobject-litegraph.bundle.esm.min.js",
      "require": "./dist/mobject-litegraph.bundle.cjs.min.js",
      "default": "./dist/mobject-litegraph.bundle.min.js"
    },
    "./full": {
      "import": "./dist/mobject-litegraph.bundle.esm.js",
      "require": "./dist/mobject-litegraph.bundle.cjs.js",
      "default": "./dist/mobject-litegraph.bundle.js"
    }
  },
  "scripts": {
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mobject-Dev-Team/mobject-litegraph"
  },
  "author": "benhar-dev",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/Mobject-Dev-Team/mobject-litegraph/issues"
  },
  "homepage": "https://github.com/Mobject-Dev-Team/mobject-litegraph#readme",
  "keywords": [
    "flow",
    "graph",
    "editor",
    "web"
  ],
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rollup": "^2.79.2",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-license": "^3.5.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
